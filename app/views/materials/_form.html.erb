<%= simple_form_for(@material) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :project %>
    <%= f.input :object %>
    <%= f.input :where_to_buy %>
    <%= f.input :amount %>
    <%= f.input :per_student %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
